{
   "name": "simple-jsonschema-ts",
   "main": "dist/index.js",
   "module": "dist/index.js",
   "types": "dist/index.d.ts",
   "version": "0.1.1",
   "description": "A radically simple and lightweight (1kb minified) TypeScript library for defining JSON schemas with static type inference. Nothing more, nothing less.",
   "repository": {
      "type": "git",
      "url": "https://github.com/sinclairzx81/typebox"
   },
   "publishConfig": {
      "access": "public"
   },
   "type": "module",
   "license": "MIT",
   "scripts": {
      "test:unit": "bun test",
      "types": "tsc --noEmit",
      "test": "bun types && bun test:unit",
      "test:spec": "clear && bun run src/test/spec/run.ts",
      "build": "tsup",
      "fetch": "npx giget gh:json-schema-org/JSON-Schema-Test-Suite/tests src/test/spec/lib",
      "prepublishOnly": "bun test && bun run build"
   },
   "devDependencies": {
      "@cfworker/json-schema": "^4.1.1",
      "@types/bun": "latest",
      "ajv": "^8.17.1",
      "bun-types": "^1.2.9",
      "expect-type": "^1.2.1",
      "hono": "^4.7.9",
      "json-schema-library": "^10.0.0",
      "picocolors": "^1.1.1",
      "tsup": "^8.4.0"
   },
   "peerDependencies": {
      "typescript": "^5.0.0"
   },
   "tsup": {
      "entry": [
         "src/lib/index.ts"
      ],
      "format": "esm",
      "platform": "neutral",
      "dts": true,
      "minify": true,
      "clean": true,
      "outDir": "dist"
   },
   "files": [
      "dist",
      "README.md"
   ],
   "keywords": [
      "json",
      "schema",
      "jsonschema",
      "json-schema",
      "typescript"
   ]
}
